{
  "name": "estree-walk-plus",
  "version": "0.0.1-alpha.4",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./build/index.js",
      "types": "./build/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:ts": "tsc",
    "build:node": "esbuild ./src/index.ts --bundle --outfile=./build/index.js --platform=node --format=cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir ./build",
    "build": "npm run build:ts && npm run build:node && npm run build:types",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "acorn": "^8.12.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "esbuild": "0.25.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "estree",
    "AST",
    "walk",
    "traverse",
    "parse-tree",
    "acorn",
    "acorn-walk"
  ],
  "author": "ahmed.s.elafifi@gmail.com",
  "license": "ISC"
}

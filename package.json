{
  "name": "estree-walk-plus",
  "version": "0.0.1-alpha.2",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./build/index.js",
      "types": "./build/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:tsc": "tsc",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir ./build",
    "build:node": "esbuild ./src/index.ts --bundle --outfile=./build/index.js --platform=node --format=cjs",
    "build": "npm run build:tsc && npm run build:types && npm run build:node",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "acorn": "^8.12.1"
  },
  "devDependencies": {
    "esbuild": "0.25.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "estree",
    "AST",
    "walk",
    "traverse",
    "parse-tree",
    "acorn",
    "acorn-walk"
  ],
  "author": "ahmed.s.elafifi@gmail.com",
  "license": "ISC"
}
